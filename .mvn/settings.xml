<profiles>
<profile>
    <id>ai.platon</id>
    <!--<name>ai.platon</name>-->
    <repositories>
        <repository>
            <id>ai.platon</id>
            <name>ai.platon</name>
            <url>https://mvnrepository.com/artifact/ai.platon/</url>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
                <checksumPolicy>warn</checksumPolicy>
            </releases>
            <snapshots>
                <enabled>true</enabled>
                <updatePolicy>never</updatePolicy>
                <checksumPolicy>fail</checksumPolicy>
            </snapshots>
        </repository>
    </repositories>
</profile>

<profile>
    <id>ai.platon.pulsar</id>
    <repository>
        <id>ai.platon.pulsar</id>
        <name>ai.platon.pulsar</name>
        <url>https://mvnrepository.com/artifact/ai.platon.pulsar/</url>
        <releases>
            <enabled>true</enabled>
        </releases>
        <snapshots>
            <enabled>false</enabled>
        </snapshots>
    </repository>
</profile>

<profile>
    <id>spring-boot</id>
    <repositories>
        <repository>
            <id>org.springframework.boot</id>
            <name>org.springframework.boot</name>
            <url>https://mvnrepository.com/artifact/org.springframework.boot/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>
</profile>

<profile>
    <id>kotlin</id>
    <!--<name>kotlin</name>-->
    <repositories>
        <repository>
            <id>kotlin</id>
            <name>kotlin</name>
            <url>https://mvnrepository.com/artifact/org.jetbrains.kotlin/</url>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
                <checksumPolicy>warn</checksumPolicy>
            </releases>
            <snapshots>
                <enabled>true</enabled>
                <updatePolicy>never</updatePolicy>
                <checksumPolicy>fail</checksumPolicy>
            </snapshots>
        </repository>
    </repositories>
</profile>

